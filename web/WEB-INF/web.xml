<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ServletMongoDB</servlet-name>
        <servlet-class>ServletMongoDB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetExam</servlet-name>
        <servlet-class>GetExam</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMongoDBCorregir</servlet-name>
        <servlet-class>ServletMongoDBCorregir</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletMongoDB</servlet-name>
        <url-pattern>/ServletMongoDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetExam</servlet-name>
        <url-pattern>/GetExam</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMongoDBCorregir</servlet-name>
        <url-pattern>/ServletMongoDBCorregir</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
